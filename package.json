{
  "name": "arbolus-ui-components",
  "license": "proprietary",
  "version": "0.0.0-development",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "vite --port 3000",
    "build": "tsc && vite build",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=fd430b667236",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "spellcheck": "cspell \"src/**/{*.ts}\" --config cspell.json"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "material-symbols": "^0.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "sass": "^1.66.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-viewport": "^7.4.0",
    "@storybook/addons": "^7.4.0",
    "@storybook/builder-vite": "^7.4.0",
    "@storybook/jest": "^0.2.2",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.4.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.4",
    "chromatic": "^6.23.0",
    "cspell": "^6.31.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "path": "^0.12.7",
    "prettier": "^2.8.4",
    "semantic-release": "^21.0.2",
    "storybook": "^7.4.0",
    "typescript": "^5.0.4",
    "vite": "^4.4.6",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.3.1"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits",
        "releaseRules": [
          {
            "type": "*!",
            "release": "major"
          }
        ]
      }
    ],
    "@semantic-release/changelog",
    "@semantic-release/npm",
    "@semantic-release/git",
    [
      "@semantic-release/github",
      {
        "successComment": false,
        "failTitle": false
      }
    ]
  ],
  "preset": "react",
  "readme": "ERROR: No README data found!",
  "repository": {
    "type": "git",
    "url": "https://github.com/arbolus-technologies/arbolus-components.git"
  },
  "packageManager": "yarn@3.6.3"
}
